<style>
    /* Modal Styling */
    .modal {
        display: none;
        position: fixed;
        z-index: 1000;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        background: rgba(0, 0, 0, 0.6);
        justify-content: center;
        align-items: center;
    }

    .modal-content {
        background: #fff;
        border-radius: 12px;
        padding: 30px;
        width: 90%;
        max-width: 400px;
        text-align: center;
        box-shadow: 0 15px 30px rgba(0, 0, 0, 0.2);
        position: relative;
    }

        .modal-content h2 {
            color: #2e7d32; /* Dark green */
            margin-bottom: 20px;
        }

        .modal-content input[type="number"] {
            width: 100%;
            padding: 12px;
            margin-bottom: 15px;
            border: 2px solid #c8e6c9;
            border-radius: 8px;
            font-size: 16px;
        }

        .modal-content button {
            background-color: #2e7d32;
            color: white;
            padding: 12px 20px;
            border: none;
            border-radius: 8px;
            cursor: pointer;
            font-size: 16px;
            transition: background-color 0.3s ease;
        }

            .modal-content button:hover {
                background-color: #1b5e20;
            }

    .close-btn {
        position: absolute;
        right: 25px;
        top: 15px;
        font-size: 28px;
        font-weight: bold;
        cursor: pointer;
        color: #888;
    }

        .close-btn:hover {
            color: #000;
        }
</style>
    <script>
        const modal = document.getElementById("paymentModal");
        const closeBtn = document.querySelector(".close-btn");
        const title = document.getElementById("paymentTitle");
        let selectedPaymentName = "";

        document.querySelectorAll(".namaz-time h4").forEach(item => {
            item.addEventListener("click", () => {
                selectedPaymentName = item.textContent.trim();
                title.textContent = `Pay for ${selectedPaymentName}`;
                modal.style.display = "flex";
            });
        });

        closeBtn.onclick = () => {
            modal.style.display = "none";
        };

        window.onclick = (event) => {
            if (event.target === modal) {
                modal.style.display = "none";
            }
        };

        document.getElementById("paymentForm").addEventListener("submit", async function (e) {
            e.preventDefault();
            const amount = parseFloat(document.getElementById("amount").value);

            if (amount && amount > 0 && selectedPaymentName) {
                try {
                    const response = await fetch('/api/payment/create-checkout-session', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                        body: JSON.stringify({
                            paymentName: selectedPaymentName,
                            amount: amount
                        })
                    });

                    // First check if response is OK (status 200-299)
                    if (!response.ok) {
                        const errorText = await response.text(); // fallback if not JSON
                        throw new Error(errorText || "Unexpected error occurred.");
                    }

                    let data;
                    try {
                        data = await response.json();
                    } catch (jsonError) {
                        throw new Error("Invalid JSON returned from server.");
                    }

                    if (data.url) {
                        window.location.href = data.url; // Redirect to Stripe Checkout
                    } else {
                        alert("There was an issue processing your payment.");
                    }

                } catch (error) {
                    alert("Error: " + error.message);
                }
            } else {
                alert("Please enter a valid amount.");
            }
        });
    </script>


